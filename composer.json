{
    "name": "jsor/string-formatter",
    "description": "Localized formatting of personal names, addresses and telephone numbers.",
    "keywords": [
        "string",
        "name",
        "address",
        "postal",
        "telephone",
        "phone",
        "number",
        "format",
        "formatting",
        "intl",
        "locale",
        "localized",
        "i18n"
    ],
    "authors": [
        {
            "name": "Jan Sorgalla",
            "email": "jsorgalla@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lookkoz/locale-data"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jsor\\StringFormatter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jsor\\StringFormatter\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1 || ^8.0",
        "jsor/locale-data": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "vimeo/psalm": "^3.4",
        "psalm/plugin-phpunit": "^0.6.0"
    }
}
